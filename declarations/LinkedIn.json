{
  "name": "LinkedIn",
  "terms": {
    "Privacy Policy": {
      "fetch": "https://fr.linkedin.com/legal/privacy-policy",
      "select": [
        "#component-container"
      ],
      "remove": [
        ".callout > svg",
        "[aria-controls=\"expandableSection\"]",
        ".banner__image-container "
      ]
    },
    "Terms of Service": {
      "fetch": "https://fr.linkedin.com/legal/l/service-terms",
      "select": [
        "main"
      ]
    },
    "Community Guidelines": {
      "fetch": "https://fr.linkedin.com/legal/professional-community-policies",
      "select": [
        "main"
      ],
      "remove": [
        ".banner__image-container",
        ".component-standaloneImage"
      ]
    }
  }
}
